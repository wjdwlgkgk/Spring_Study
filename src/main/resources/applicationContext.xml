<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns="http://www.springframework.org/schema/beans"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.springframework.org/schema/beans
                            http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="greeter" class="chap02.Greeter">
        <property name="format" value="%s, 안녕하세요!"/>
    </bean>

    <bean id="memberDao" class="chap03.Data.MemberDao">
    </bean>

    <bean id="memberRegSvc" class="chap03.RegisterService.MemberRegisterService">
        <constructor-arg ref="memberDao"/>
    </bean>

    <bean id="changePwdSvc" class="chap03.PasswordChangeService.ChangePasswordService">
        <constructor-arg ref="memberDao"/>
    </bean>

    <bean id="listPrinter" class="chap03.print.MemberListPrinter">
        <constructor-arg ref="memberDao"/>
        <constructor-arg ref="memberPrinter"/>
    </bean>

    <bean id="memberPrinter" class="chap03.print.MemberPrinter"></bean>

</beans>